let raceNumber = Math.floor(Math.random() * 1000);

const registeredEarly = false ;
const age = 33;

if(registeredEarly === true && age >= 18){
  raceNumber + 1000;
  console.log('The race will start at 9:30am and your number is ${raceNumber}');
} else if (registeredEarly === false && age >= 18){
  console.log(`The race will start at 11:00am and your number is ${raceNumber}`);
} else if (age < 18){
  console.log(`Your race will begin at 12:30 and your number is ${raceNumber}`)
}
